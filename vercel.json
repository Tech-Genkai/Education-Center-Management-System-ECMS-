{
  "version": 2,
  "buildCommand": "npm install --prefix ./shared && npm install --prefix ./backend && npm run build --prefix ./backend",
  "devCommand": "cd backend && npm run dev",
  "installCommand": "echo 'Skip auto-install'",
  "framework": null,
  "outputDirectory": "backend/dist",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/backend/public/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
